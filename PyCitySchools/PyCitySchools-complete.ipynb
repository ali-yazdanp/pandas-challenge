   "source": [
    "# Use 4 reasonable bins to group school spending.\n",
    "School_Size_bins = [0, 1000, 2000, 5000]\n",
    "School_Size_bins_names = [\"Small (<1000)\", \"Medium (1000-2000)\", \"Large (2000-5000)\"]\n",
    "\n",
    "# Create a table that breaks down school performances based on school size\n",
    "# Add school sizes by the defined school sizes bins\n",
    "school_summary_df[\"School Size\"] = pd.cut(school_summary_df[\"Total Students\"], School_Size_bins, labels=School_Size_bins_names)\n",
    "# Groupby Spending ranges\n",
    "School_Sizes_df = school_summary_df.groupby([\"School Size\"])              \n",
    "# Calculate the values for the data table\n",
    "Scores_by_School_Sizes_df = School_Sizes_df.mean()\n",
    "\n",
    "# Formatting\n",
    "Scores_by_School_Sizes_df[\"Average Maths Score\"] = Scores_by_School_Sizes_df[\"Average Maths Score\"].map(\"{:,.2f}\".format)\n",
    "Scores_by_School_Sizes_df[\"Average Reading Score\"] = Scores_by_School_Sizes_df[\"Average Reading Score\"].map(\"{:,.2f}\".format)\n",
    "Scores_by_School_Sizes_df[\"% Passing Maths\"] = Scores_by_School_Sizes_df[\"% Passing Maths\"].map(\"{:,.2f}\".format)\n",
    "Scores_by_School_Sizes_df[\"% Passing Reading\"] = Scores_by_School_Sizes_df[\"% Passing Reading\"].map(\"{:,.2f}\".format)\n",
    "Scores_by_School_Sizes_df[\"% Overall Passing\"] = Scores_by_School_Sizes_df[\"% Overall Passing\"].map(\"{:,.2f}\".format)\n",
    "\n",
    "# Display table\n",
    "Scores_by_School_Sizes_df[[\"Average Maths Score\",\n",
    "                    \"Average Reading Score\",\n",
    "                    \"% Passing Maths\",\n",
    "                    \"% Passing Reading\",\n",
    "                    \"% Overall Passing\"]]"
   ]
