    "NumStudentPassingMaths = school_data_complete[school_data_complete[\"maths_score\"] >= 50].groupby([\"school_name\"])[\"maths_score\"].count()\n",
    "PercentagePassingMaths = NumStudentPassingMaths / TotStudents * 100\n",
    "# % passing reading (the percentage of students who passed reading)\n",
    "NumStudentPassingReading = school_data_complete[school_data_complete[\"reading_score\"] >= 50].groupby([\"school_name\"])[\"reading_score\"].count()\n",
    "PercentagePassingReading = NumStudentPassingReading / TotStudents * 100\n",
    "# % overall passing (the percentage of students who passed maths AND reading)\n",
    "OverallPassing = school_data_complete[(school_data_complete[\"maths_score\"] >= 50) & (school_data_complete[\"reading_score\"] >= 50)]\n",
    "OverallPassingBoth = OverallPassing.groupby([\"school_name\"])\n",
    "PercentageOverallPassing =(OverallPassingBoth[\"reading_score\"].count()/TotStudents) * 100\n",
    "\n",
    "# create a DataFrame that summarises key metrics about each school.\n",
    "school_summary_df = pd.DataFrame({\"School Type\": schoolType,\n",
