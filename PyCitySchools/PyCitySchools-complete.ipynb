    "# Total number of unique schools\n",
    "TotSchools = school_data_complete[\"school_name\"].nunique()\n",
    "\n",
    "# Total number of students\n",
    "TotStudents = school_data_complete[\"Student ID\"].nunique()\n",
    "\n",
    "# Total budget\n",
    "TotBudget = school_data[\"budget\"].sum()\n",
    "\n",
    "# % passing maths (the percentage of students who passed maths)\n",
    "AvgMathsScore = school_data_complete[\"maths_score\"].mean()\n",
    "NumStudentPassingMaths = len(school_data_complete[school_data_complete[\"maths_score\"] >= 50])\n",
    "PercentagePassingMaths = NumStudentPassingMaths / TotStudents * 100\n",
    "\n",
    "# % passing reading (the percentage of students who passed reading)\n",
    "AvgReadingScore = school_data_complete[\"reading_score\"].mean()\n",
    "NumStudentPassingReading = len(school_data_complete[school_data_complete[\"reading_score\"] >= 50])\n",
    "PercentagePassingReading = NumStudentPassingReading / TotStudents * 100\n",
    "\n",
    "# % overall passing (the percentage of students who passed maths AND reading)\n",
    "OverallPassing = school_data_complete[(school_data_complete[\"maths_score\"] >= 50) & (school_data_complete[\"reading_score\"] >= 50)].count()[\"Student ID\"]\n",
    "PercentageOverallPassing = OverallPassing/TotStudents * 100\n",
    "\n",
    "# create a high-level snapshot of the local government area's key metrics in a DataFrame\n",
    "\n",
    "local_government_df = pd.DataFrame({\n",
    "    \"Total Schools\": [TotSchools],\n",
    "    \"Total Students\": [TotStudents],\n",
    "    \"Total Budget\": [TotBudget],\n",
    "    \"Average Maths Score\": [AvgMathScore],\n",
    "    \"Average Reading Score\": [AvgReadingScore],\n",
    "    \"% Passing Math\": [PercentagePassingMath],\n",
    "    \"% Passing Reading\": [PercentageOverallPassing],\n",
    "    \"% Overall Passing \": [PercentageOverallPassing]\n",
    "})\n",
    "\n",
    "# Formatting \"Total Students\" and \"Total Budget\"\n",
    "local_government_df[\"Total Budget\"] = local_government_df[\"Total Budget\"].map(\"${:,.2f}\".format)\n",
    "local_government_df[\"Total Students\"] = local_government_df[\"Total Students\"].map(\"{:,.0f}\".format)\n",
